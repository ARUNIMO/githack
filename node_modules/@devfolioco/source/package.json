{
  "name": "@devfolioco/source",
  "version": "1.0.0-beta.30",
  "description": "Devfolio's Design System",
  "repository": {
    "type": "git",
    "url": "https://github.com/devfolioco/genesis.git"
  },
  "license": "MIT",
  "author": "Devfolio",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node generateIcons.js && rollup -c",
    "build:storybook": "build-storybook -s ./assets",
    "lint": "prettier --list-different \"**/*.{js,json,css,md,ts,tsx}\"",
    "lint:fix": "prettier --write \"**/*.{js,json,css,md,ts,tsx}\"",
    "prepublishOnly": "rm -rf dist && yarn build",
    "snapshot": "build-storybook && percy-storybook --widths=700,1280",
    "start": "yarn storybook",
    "storybook": "start-storybook -s ./assets -p 8081 -c .storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "framer-motion": "3.10.0",
    "react-popper": "2.2.4"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@percy/storybook": "3.3.1",
    "@popperjs/core": "2.9.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@rollup/plugin-url": "6.0.0",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/addon-viewport": "6.1.21",
    "@storybook/addons": "6.1.21",
    "@storybook/react": "6.1.21",
    "@storybook/theming": "6.1.21",
    "@svgr/core": "5.5.0",
    "@svgr/plugin-prettier": "5.5.0",
    "@svgr/plugin-svgo": "5.5.0",
    "@svgr/rollup": "5.5.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-select": "3.0.25",
    "@types/storybook__addon-info": "5.2.3",
    "@types/styled-components": "5.1.7",
    "babel-loader": "8.2.2",
    "fs-extra": "9.1.0",
    "glob": "7.1.6",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "lodash.camelcase": "4.3.0",
    "lodash.upperfirst": "4.3.1",
    "prettier": "2.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "17.0.1",
    "rollup": "2.40.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "styled-components": "5.2.1",
    "typescript": "4.2.3"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=5.1.1"
  }
}
